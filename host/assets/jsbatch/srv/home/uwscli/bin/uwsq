#!/bin/sh
set -eu
export NQDIR=/run/uwscli/nq
mkdir -p -m 0750 ${NQDIR}
cd ${NQDIR}
exec sudo -H -n -u uws -- fq "$@"

#uwscli:uwsq				- show information about uws jobs queue

#uwsdoc:uwsq
#uwsdoc:
#uwsdoc:  It will show current running job output or information about last
#uwsdoc:  failed job. If there's no output, there are no running jobs nor
#uwsdoc:  any that failed either.
