import sys
sys.path.insert(0, '/srv/home/uwscli/lib')

import custom_deploy

__doc__ = 'tapo staging custom deploy status'

cfg = custom_deploy.Config(
	app_name = 'app',
	app_env  = 'staging',
)

if __name__ == '__main__':
	sys.stdout.reconfigure(line_buffering = False)
	sys.stderr.reconfigure(line_buffering = False)
	sys.exit(custom_deploy.status(sys.argv[1:], cfg))
