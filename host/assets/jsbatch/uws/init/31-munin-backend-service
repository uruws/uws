#!/bin/sh
set -eu


# 231218 disabled by jrms
set +e
systemctl stop uws-munin-backend
if systemctl is-enabled uws-munin-backend.service; then
	systemctl disable uws-munin-backend.service
fi
exit 0


sudo -n -u uws -D /srv/uws/deploy make munin-backend

if ! systemctl is-enabled uws-munin-backend.service; then
	systemctl enable uws-munin-backend.service
	systemctl start uws-munin-backend
fi

export DOCKER_IMAGE='uws/munin-backend-2309'

/uws/bin/service-restart.sh uws-munin-backend /uws/munin \
	/etc/systemd/system/uws-munin-backend.service /srv/etc/munin

exit 0
