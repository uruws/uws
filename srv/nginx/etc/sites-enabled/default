server {
	listen 80      default_server;
	listen [::]:80 default_server;

	server_name _;

	root /var/www/html;
	index index.html index.nginx-debian.html;

	include snippets/uwslog.conf;

	location /healthz {
		access_log off;
		return 200;
	}

	location / {
		return 301 https://$host$request_uri;
	}
}
